#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x561bef71c570 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x561bef72d900_0 .var "clk", 0 0;
v0x561bef72d9d0_0 .net "count", 3 0, v0x561bef72d700_0;  1 drivers
v0x561bef72daa0_0 .var "reset", 0 0;
S_0x561bef71c700 .scope module, "counter" "up_counter" 2 7, 3 1 0, S_0x561bef71c570;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "count";
v0x561bef6e4ba0_0 .net "clk", 0 0, v0x561bef72d900_0;  1 drivers
v0x561bef72d700_0 .var "count", 3 0;
v0x561bef72d7e0_0 .net "reset", 0 0, v0x561bef72daa0_0;  1 drivers
E_0x561bef71cb60 .event posedge, v0x561bef72d7e0_0, v0x561bef6e4ba0_0;
    .scope S_0x561bef71c700;
T_0 ;
    %wait E_0x561bef71cb60;
    %load/vec4 v0x561bef72d7e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x561bef72d700_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x561bef72d700_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x561bef72d700_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x561bef71c570;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "simulation.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x561bef71c570 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561bef72d900_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561bef72daa0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x561bef72daa0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x561bef72daa0_0, 0, 1;
    %pushi/vec4 40, 0, 32;
T_1.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_1.1, 5;
    %jmp/1 T_1.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %delay 5, 0;
    %load/vec4 v0x561bef72d900_0;
    %inv;
    %store/vec4 v0x561bef72d900_0, 0, 1;
    %jmp T_1.0;
T_1.1 ;
    %pop/vec4 1;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    "up_counter.v";
