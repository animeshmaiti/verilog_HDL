#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x563d5eede560 .scope module, "upcounter_test" "upcounter_test" 2 23;
 .timescale -9 -12;
v0x563d5eeeeee0_0 .var "clock", 0 0;
v0x563d5eeeefb0_0 .net "count", 3 0, v0x563d5eea6ba0_0;  1 drivers
v0x563d5eeef080_0 .var "reset", 0 0;
S_0x563d5eede6f0 .scope module, "c0" "upcounter" 2 26, 3 23 0, S_0x563d5eede560;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clock";
    .port_info 1 /OUTPUT 4 "count";
    .port_info 2 /INPUT 1 "reset";
v0x563d5eea67b0_0 .net "clock", 0 0, v0x563d5eeeeee0_0;  1 drivers
v0x563d5eea6ba0_0 .var "count", 3 0;
v0x563d5eeeedc0_0 .net "reset", 0 0, v0x563d5eeef080_0;  1 drivers
E_0x563d5eea56d0 .event posedge, v0x563d5eea67b0_0;
    .scope S_0x563d5eede6f0;
T_0 ;
    %wait E_0x563d5eea56d0;
    %load/vec4 v0x563d5eeeedc0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x563d5eea6ba0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x563d5eea6ba0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x563d5eea6ba0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x563d5eede560;
T_1 ;
    %vpi_call 2 29 "$dumpfile", "simulation.vcd" {0 0 0};
    %vpi_call 2 30 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x563d5eede560 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563d5eeeeee0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563d5eeef080_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563d5eeef080_0, 0, 1;
    %delay 160000, 0;
    %vpi_call 2 34 "$stop" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x563d5eede560;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x563d5eeeeee0_0;
    %inv;
    %store/vec4 v0x563d5eeeeee0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "upcounter_test.v";
    "upcounter.v";
